{
  // Required
  "manifest_version": 2,
  "name": "De-wordle",
  "version": "1",

  // Recommended
  "description": "de-wordle, suggestion generator",
  "browser_action": {
    "default_popup":"popup.html",
    "default_icon":"icon.png"
  },
  "permissions": [
    "activeTab",
    "contextMenus"
  ]


}
